{
  "name": "arbre",
  "version": "0.0.0",
  "description": "Generic tree library nature lovers.",
  "author": "Nicolas Gryman <ngryman@gmail.com> (http://ngryman.sh/)",
  "license": "MIT",
  "repository": "ngryman/arbre",
  "main": "index.cjs",
  "jsnext:main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "index.js",
    "index.cjs",
    "lib/"
  ],
  "scripts": {
    "lint": "eslint packages/*/**/*.js",
    "pretest": "npm run lint -s",
    "test": "nyc ava packages/*/test",
    "dev": "ava packages/*/test --watch",
    "coverage": "meta coverage",
    "see-coverage": "meta see-coverage",
    "check-coverage": "meta check-coverage",
    "docs": "meta docs",
    "prebuild": "npm run lint -s",
    "build": ""
  },
  "precommit": [
    "test",
    "check-coverage",
    "build",
    "docs"
  ],
  "keywords": [
    "tree",
    "node",
    "manipulation",
    "mutable",
    "mutation",
    "algorithm",
    "traversal",
    "walk",
    "depth first",
    "breath first"
  ],
  "dependencies": {
    "clone": "^1.0.2",
    "is-plain-obj": "^1.1.0",
    "not-so-shallow": "^0.1.4",
    "tree-crawl": "^0.2.0",
    "tree-morph": "^0.1.0",
    "tree-mutate": "^0.2.1"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^2.0.1",
    "lerna": "2.0.0-beta.28",
    "meta-dev": "^0.1.0",
    "pre-commit": "^1.1.3",
    "rollup-plugin-multi-entry": "^2.0.1"
  },
  "ava": {
    "concurrency": 4,
    "require": [
      "babel-register",
      "./test/helpers/seed"
    ],
    "babel": "inherit"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs",
      "istanbul"
    ],
    "ignore": "packages/*/test/*.js",
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "nyc": {
    "include": [
      "packages/*/lib/*.js"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
