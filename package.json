{
  "name": "oaks",
  "version": "0.0.0",
  "description": "Versioned trees for green thumbs",
  "author": "Nicolas Gryman <ngryman@gmail.com> (http://ngryman.sh/)",
  "license": "MIT",
  "repository": "ngryman/oaks",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "eslint {,lib/,test/}*.js",
    "unit": "nyc ava",
    "test": "npm run lint -s && npm run unit -s",
    "dev": "npm run unit -- --watch",
    "coverage": "nyc report --reporter=text-lcov | codecov",
    "see-coverage": "nyc report --reporter=html && open coverage/index.html",
    "check-coverage": "nyc check-coverage --lines 95 --functions 95 --branches 95"
  },
  "precommit": [
    "test",
    "check-coverage"
  ],
  "keywords": [],
  "dependencies": {
    "cuid": "^1.3.8",
    "hamt": "^2.2.0",
    "m-sorted-array": "^1.1.3"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-preset-node6": "^11.0.0",
    "codecov.io": "^0.1.6",
    "eslint": "^3.1.1",
    "eslint-config-ngryman": "^1.4.0",
    "nyc": "^8.0.0",
    "pre-commit": "^1.1.3"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "node6"
    ]
  }
}
